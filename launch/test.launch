<?xml version="1.0" encoding="UTF-8"?>
<launch>

  <!--include file="$(find fetch_gazebo)/launch/simulation.launch" /-->
  <arg name="robot" default="fetch"/>
  <arg name="debug" default="false"/>
  <arg name="gui" default="true"/>
  <arg name="headless" default="false"/>
  <!-- empty map -->
  <include file="$(find jsk_maps)/launch/start_empty_map.launch"/>
  <node pkg="tf" type="static_transform_publisher" name="$(anon map_tf01)"
      args="  0.000   0.000   0.000 0 0 0 /map /base_link 100" />

  <!-- Start Gazebo with a blank world -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="debug" value="$(arg debug)" />
    <arg name="gui" value="$(arg gui)" />
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="headless" value="$(arg headless)"/>
    <arg name="world_name" value="$(find touch_localization)/tabletop.world"/>
  </include>

  <!-- Oh, you wanted a robot? -->
  <include file="$(find fetch_gazebo)/launch/include/$(arg robot).launch.xml" />

  <param name="object_description" textfile="$(find touch_localization)/urdf/kettle.urdf"/>
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-param object_description -urdf -x 1.4 -y 0.0 -z 0.11 -R 0.0 -P 0.0 -Y 0.4 -model kettle" />
</launch>
